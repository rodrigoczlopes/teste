{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "globals": {
        "use": true,
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
      "prettier/prettier": ["error"],
      "class-methods-use-this": "off",
      "consistent-return": "off",
      "no-param-reassign": "off",
      "camelcase":"off"
    }
}
